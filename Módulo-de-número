using System;
					
public class Program
{
	public static void Main()
	{
	    int A = 0;
        Console.WriteLine("Digite um valor: ");
        A = Convert.ToInt32(Console.ReadLine());
        if (A > 0)
        {
            Console.WriteLine("O módulo é " + A);
        }
        else
        {
            Console.WriteLine("O módulo é " + -1*A);
		}
	}
}
